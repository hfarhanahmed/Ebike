{
  "name": "Ebike",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "test": "jest",
    "lint": "eslint .",
    "flow": "flow",
    "start": "react-native start",
    "ios": "cd ios && pod install && cd .. && react-native run-ios",
    "ios:stg": " cd ios && pod install && cd .. && react-native run-ios --scheme 'Ebike_Staging'",
    "ios:prod": " cd ios && pod install && cd .. && react-native run-ios --scheme 'Ebike_Production'",
    "android": "react-native run-android",
    "clean:android": "rm -rf android/build",
    "clean:ios": "rm -rf ios/build && rm -rf ios/pods",
    "clean:all": "rm -rf node_modules && rm -rf yarn.lock  && yarn cache clean && killall node",
    "deepclean": "watchman watch-del-all && rm -rf node_modules && rm -rf $TMPDIR/haste-map-* && rm -rf $TMPDIR/metro-* && rm -rf $TMPDIR/react-* && yarn cache clean && rm -rf yarn.lock && rm -rf ios/build && rm -rf ios/pods",
    "generate:apk": "cd android && sh PackageAndroid.sh"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.13.2",
    "@react-navigation/native": "^5.9.4",
    "@react-navigation/stack": "^5.14.5",
    "appcenter": "4.0.2",
    "appcenter-analytics": "4.0.2",
    "appcenter-crashes": "4.0.2",
    "axios": "^0.21.1",
    "i18n-js": "^3.8.0",
    "lodash": "^4.17.21",
    "react": "17.0.1",
    "react-native": "0.64.0",
    "react-native-cli": "^2.0.1",
    "react-native-code-push": "^7.0.0",
    "react-native-config": "^1.4.2",
    "react-native-gesture-handler": "^1.10.3",
    "react-native-image-slider-box": "^1.0.12",
    "react-native-safe-area-context": "^0.7.3",
    "react-native-splash-screen": "^3.2.0",
    "react-native-vector-icons": "^8.1.0",
    "simplicity": "^1.1.3"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^2.0.0",
    "babel-jest": "^26.6.3",
    "eslint": "7.14.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "flow-bin": "0.137.0",
    "jest": "^26.6.3",
    "metro-react-native-babel-preset": "^0.64.0",
    "prettier": "^2.2.1",
    "react-test-renderer": "17.0.1"
  },
  "jest": {
    "preset": "react-native",
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/assetsTransformer.js",
      "\\.(css|less)$": "<rootDir>/assetsTransformer.js"
    },
    "setupFiles": [
      "./jestSetup.js"
    ]
  }
}
